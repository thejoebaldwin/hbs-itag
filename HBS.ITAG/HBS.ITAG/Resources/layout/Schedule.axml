<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">
  <RelativeLayout
      android:layout_width="fill_parent"
      android:layout_height="wrap_content"
      android:background="#FFFFFF">
    <Button
        android:id="@+id/day_one_btn"
        android:text="6/20"
        android:layout_width="192px"
        android:layout_height="wrap_content"
        android:background="@drawable/selected_day" />
    <Button
        android:id="@+id/day_two_btn"
        android:text="6/21"
        android:layout_width="192px"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/day_one_btn"
        android:layout_margin="0px"
        android:background="@drawable/unselected_day" />
    <Button
        android:id="@+id/day_three_btn"
        android:text="6/22"
        android:layout_width="192px"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/day_two_btn"
        android:background="@drawable/unselected_day" />
    <Button
        android:id="@+id/day_four_btn"
        android:text="6/23"
        android:layout_width="192px"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/day_three_btn"
        android:background="@drawable/unselected_day" />
  </RelativeLayout>
  <FrameLayout
      android:id="@+id/day_one"
      android:layout_height="920px"
      android:visibility="visible"
      android:layout_width="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_height="50px"
        android:layout_width="match_parent"
        android:background="@drawable/whiteBox">
      <ImageView
          android:id="@+id/left_arrow"
          android:src="@drawable/left_arrow"
          android:layout_width="40px"
          android:layout_height="wrap_content"
          android:layout_marginRight="0.0dp"
          android:layout_marginLeft="5.0dp"
          android:layout_marginTop="5px" />
      <TextView
          android:layout_width="335.5dp"
          android:layout_height="wrap_content"
          android:id="@+id/track_title"
          android:text="Track 2"
          android:background="@android:color/white"
          android:textColor="@android:color/black"
          android:gravity="center"
          android:layout_marginTop="7px"
          android:layout_marginBottom="10px" />
      <ImageView
          android:id="@+id/right_arrow"
          android:src="@drawable/right_arrow"
          android:layout_width="40px"
          android:layout_height="wrap_content"
          android:gravity="right"
          android:layout_marginRight="5dp"
          android:layout_marginLeft="-5dp"
          android:layout_marginTop="5px" />
    </LinearLayout>
    <ScrollView
        android:layout_height="865px"
        android:layout_width="match_parent"
        android:layout_marginTop="60px">
      <LinearLayout
          android:orientation="horizontal"
          android:layout_height="match_parent"
          android:layout_width="match_parent"
          android:layout_marginTop="20px"
          android:paddingBottom="50px">
        <TextView
            android:layout_height="wrap_content"
            android:layout_width="150px"
            android:maxLines="50"
            android:textColor="@android:color/black"
            android:gravity="right"
            android:text="8:30 am\n9:00 am\n9:30 am\n10:00 am\n10:30 am\n11:00 am\n11:30 am\n12:00 pm\n12:30 pm\n1:00 pm\n1:30 pm\n2:00 pm\n2:30 pm\n3:00 pm\n3:30 pm\n4:00 pm"
            android:includeFontPadding="true"
            android:lineSpacingExtra="50px"
            android:paddingRight="20px" />
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
          <TextView
              android:layout_height="83px"
              android:layout_width="fill_parent"
              android:text="Registration"
              android:background="@drawable/secondaryBox"
              android:textColor="@android:color/black"
              android:layout_marginTop="20px"
              android:gravity="center" />
          <TextView
              android:layout_height="250px"
              android:layout_width="fill_parent"
              android:text="First Session Title\n9:00 am - 10:30 am"
              android:background="@drawable/primaryBox"
              android:textColor="@android:color/white"
              android:gravity="center" />
          <TextView
              android:layout_height="41px"
              android:layout_width="fill_parent"
              android:text="Break"
              android:background="@drawable/secondaryBox"
              android:textColor="@android:color/black"
              android:gravity="center" />
          <TextView
              android:layout_height="209px"
              android:layout_width="fill_parent"
              android:text="Second Session Title\n10:45 am - 12:00 pm"
              android:background="@drawable/tertiaryBox"
              android:textColor="@android:color/white"
              android:gravity="center" />
          <TextView
              android:layout_height="172px"
              android:layout_width="fill_parent"
              android:text="Lunch"
              android:background="@drawable/secondaryBox"
              android:textColor="@android:color/black"
              android:gravity="center" />
          <TextView
              android:layout_height="209px"
              android:layout_width="fill_parent"
              android:text="Third Session Title\n1:00 pm - 2:15 pm"
              android:background="@drawable/primaryBox"
              android:textColor="@android:color/white"
              android:gravity="center" />
          <TextView
              android:layout_height="41px"
              android:layout_width="fill_parent"
              android:text="Break"
              android:background="@drawable/secondaryBox"
              android:textColor="@android:color/black"
              android:gravity="center" />
          <TextView
              android:layout_height="250px"
              android:layout_width="fill_parent"
              android:text="Fourth Session Title\n2:30 pm - 4:00 pm"
              android:background="@drawable/tertiaryBox"
              android:textColor="@android:color/white"
              android:gravity="center" />
        </LinearLayout>
      </LinearLayout>
    </ScrollView>
  </FrameLayout>
